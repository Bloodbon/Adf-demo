{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "sourcesales"
				}
			],
			"sinks": [
				{
					"name": "SNOWFLAKE"
				}
			],
			"transformations": [
				{
					"name": "filter1"
				},
				{
					"name": "aggregate"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ORDERNUMBER as short,",
				"          QUANTITYORDERED as short,",
				"          PRICEEACH as double,",
				"          ORDERLINENUMBER as short,",
				"          SALES as double,",
				"          ORDERDATE as date,",
				"          STATUS as string,",
				"          QTR_ID as short,",
				"          MONTH_ID as short,",
				"          YEAR_ID as short",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false) ~> sourcesales",
				"sourcesales filter(SALES>=2000) ~> filter1",
				"filter1 aggregate(groupBy(YEAR_ID,",
				"          MONTH_ID),",
				"     TOTALSALES = sum(SALES)) ~> aggregate",
				"aggregate sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     format: 'table') ~> SNOWFLAKE"
			]
		}
	}
}