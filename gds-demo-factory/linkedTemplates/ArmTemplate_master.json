{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "gds-demo-factory"
		},
		"AzureSynapseAnalytics1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSynapseAnalytics1'"
		},
		"CosmosDbNoSql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosDbNoSql'"
		},
		"Snowflaketemp_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Snowflaketemp'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://airbnbpro.dfs.core.windows.net/"
		},
		"AzureSynapseAnalytics1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "awhs.sql.azuresynapse.net"
		},
		"AzureSynapseAnalytics1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "airbnb"
		},
		"AzureSynapseAnalytics1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		},
		"Snowflaketemp_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sales_mart"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "gds-demo-factory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSynapseAnalytics1_password": {
						"value": "[parameters('AzureSynapseAnalytics1_password')]"
					},
					"CosmosDbNoSql_connectionString": {
						"value": "[parameters('CosmosDbNoSql_connectionString')]"
					},
					"Snowflaketemp_password": {
						"value": "[parameters('Snowflaketemp_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureSynapseAnalytics1_properties_typeProperties_server": {
						"value": "[parameters('AzureSynapseAnalytics1_properties_typeProperties_server')]"
					},
					"AzureSynapseAnalytics1_properties_typeProperties_database": {
						"value": "[parameters('AzureSynapseAnalytics1_properties_typeProperties_database')]"
					},
					"AzureSynapseAnalytics1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSynapseAnalytics1_properties_typeProperties_userName')]"
					},
					"Snowflaketemp_properties_typeProperties_database": {
						"value": "[parameters('Snowflaketemp_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}